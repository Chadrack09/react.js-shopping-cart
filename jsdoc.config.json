{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["./src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "better-docs": {
      "name": "React components"
    },
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "readme": "README.md"
  }
}